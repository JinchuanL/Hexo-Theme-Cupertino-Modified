<nav id="theme-nav">
    <div class="inner">
        <a class="title" href="<%= config.root %>"><%= theme.nav.title %></a>
        <div class="nav-arrow"></div>
        <div class="nav-items">
            <a class="nav-item nav-item-home" href="<%= config.root %>"><%= __('home') %></a>
            <% for (const i in theme.nav.items) { %>
            <% if (i == 'GitHub' || i == 'github' || i == 'gh') { %>
            <a class="nav-item nav-item-github nav-item-icon" href="<%- url_for(theme.nav.items[i]) %>" target="_blank" aria-label="GitHub">&nbsp;</a>
            <% } else if (i == 'LinkedIn' || i == 'linkedin'){ %>
            <a class="nav-item nav-item-linkedin nav-item-icon" href="<%- url_for(theme.nav.items[i]) %>" target="_blank">&nbsp;</a>
            <% } else if (i == 'Weibo' || i == 'weibo'){ %>
            <a class="nav-item nav-item-weibo nav-item-icon" href="<%- url_for(theme.nav.items[i]) %>" target="_blank">&nbsp;</a> 
            <% } else if (i == 'Search' || i == 'search'){ %>
            <a class="nav-item nav-item-search nav-item-icon" href="<%- url_for(theme.nav.items[i]) %>" aria-label="Search">&nbsp;</a>
            <% } else { %>
            <a class="nav-item" href="<%- url_for(theme.nav.items[i]) %>"><%= i %></a>
            <% } %>
            <% } %>
        </div>
    </div>
</nav>